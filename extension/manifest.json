{
    "name": "Hello Extensions HAHAHAH",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["tabs", "activeTab", "scripting", "webRequest"],
    "action": {
      "default_popup": "hello.html",
      "default_icon": "hello_extensions.png.png"
    },
    "background":{
      "service_worker": "background.js"
    },
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js":["content.js"]
    }],
    "host_permissions": ["*://*/*"]
  }
  